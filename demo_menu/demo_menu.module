/**
 * Implements hook_menu_link_defaults().
 */
function demo_menu_menu_link_defaults() {
  $links['info'] = array(
    'link_title' => 'Menu',
    'route_name' => 'demo_menu.info',
    'menu_name' => 'main',
  );

  return $links;
}